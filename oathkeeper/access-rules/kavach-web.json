[
  {
    "id": "kavach:web:anonymous",
    "upstream": {
      "preserve_host": true,
      "url": "http://kavach-web:3000"
    },
    "match": {
      "url": "<http://127.0.0.1:4455/.factly/kavach/web(/.*)?>",
      "methods": [
        "GET",
        "POST"
      ]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "noop"
      }
    ]
  }
]